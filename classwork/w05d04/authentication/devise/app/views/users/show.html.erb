<h1>My Post</h1>
<div class="col-sm-12 rb">
	<% @user.posts.each do |post| %>
	<div class="posts">
		<h3><%=post.title%></h3>
		<p><%=post.body%></p>
		<div>
			<%= link_to 'Show', post %> |
	        <%= link_to 'Edit', edit_post_path(post) %> |
	        <%= link_to 'Destroy', post, method: :delete, data: { confirm: 'Are you sure?' } %>
	    </div>
    </div>
	<%end%>
</div>